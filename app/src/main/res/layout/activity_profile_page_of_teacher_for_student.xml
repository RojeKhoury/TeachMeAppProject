<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".ProfilePageOfTeacherForStudent">    <FrameLayout        android:id="@+id/frameLayout10"        android:layout_width="match_parent"        android:layout_height="wrap_content"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <include layout="@layout/activity_hamburger_menu" />        <TextView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_gravity="center"            android:fontFamily="@font/montserrat_medium"            android:gravity="center"            android:text="Profile Page"            android:textColor="#FFFFFF"            android:textSize="24dp" />    </FrameLayout>    <ImageView        android:id="@+id/imageView2"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:paddingBottom="@dimen/HalfTopBarHeight"        android:scaleType="fitXY"        android:src="@drawable/profile_page_background_view"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/frameLayout10" />    <androidx.cardview.widget.CardView        android:id="@+id/cardView2"        android:layout_width="150dp"        android:layout_height="150dp"        android:layout_marginTop="32dp"        app:cardCornerRadius="250dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/frameLayout10">        <ImageView            android:id="@+id/profile_page_user_picture"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_gravity="bottom"            android:scaleType="centerCrop"            android:src="@drawable/profile_page_incogneto_mode" />    </androidx.cardview.widget.CardView>    <TextView        android:id="@+id/profile_page_teacher_name"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_gravity="center"        android:layout_marginTop="8dp"        android:gravity="center"        android:text="Teacher Name"        android:textSize="@dimen/HalfTopBarHeight"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/cardView2" />    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/sub_parent"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:layout_weight="60"        android:padding="@dimen/StandardPadding"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/buttonAboutMe">        <RatingBar            style = "?android:attr/ratingBarStyleIndicator"            android:id="@+id/profile_page_rating_bar"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="8dp"            android:isIndicator="true"            android:numStars="5"            android:saveEnabled="true"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <ImageView            android:id="@+id/profile_page_image_view_check_or_cancel_at_my_place"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_marginTop="16dp"            android:paddingHorizontal="@dimen/StandardPadding"            android:src="@drawable/check_icon"            app:layout_constraintEnd_toStartOf="@+id/textView3"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/profile_page_rating_bar" />        <TextView            android:id="@+id/textView3"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginRight="5dp"            android:layout_weight="2"            android:fontFamily="@font/montserrat_bold"            android:gravity="center"            android:paddingHorizontal="@dimen/StandardPadding"            android:text="Face-to-Face Meeting"            android:textColor="#000"            app:layout_constraintBottom_toBottomOf="@+id/profile_page_image_view_check_or_cancel_at_my_place"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0.5"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="@+id/profile_page_image_view_check_or_cancel_at_my_place" />        <Button            android:id="@+id/button_showLocation_displayProfile"            android:layout_width="wrap_content"            android:layout_height="0dp"            android:layout_weight="2.5"            android:background="@color/colorPrimaryDark"            android:fontFamily="@font/montserrat_medium"            android:gravity="center"            android:onClick="OnClick_profile_button_check_location"            android:text="Check Location"            android:textAllCaps="false"            android:textColor="#fff"            android:visibility="invisible"            app:layout_constraintBottom_toBottomOf="@+id/profile_page_image_view_check_or_cancel_at_my_place"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@+id/textView3"            app:layout_constraintTop_toTopOf="@+id/profile_page_image_view_check_or_cancel_at_my_place" />        <Button            android:id="@+id/button4"            android:layout_width="0dp"            android:layout_height="0dp"            android:layout_weight="2.5"            android:background="@color/colorPrimaryDark"            android:gravity="center"            android:text="Check Radius"            android:textColor="@color/cardview_light_background"            android:visibility="gone"            app:layout_constraintBottom_toBottomOf="@+id/profile_page_image_view_check_or_cancel_at_my_place"            app:layout_constraintEnd_toEndOf="@+id/button_showLocation_displayProfile"            app:layout_constraintStart_toStartOf="@+id/button_showLocation_displayProfile"            app:layout_constraintTop_toTopOf="@+id/profile_page_image_view_check_or_cancel_at_my_place" />        <ImageView            android:id="@+id/profile_page_image_view_check_or_cancel_zoom"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_gravity="center"            android:layout_marginTop="16dp"            android:paddingHorizontal="@dimen/StandardPadding"            android:src="@drawable/check_icon"            app:layout_constraintEnd_toEndOf="@+id/profile_page_image_view_check_or_cancel_at_my_place"            app:layout_constraintStart_toStartOf="@+id/profile_page_image_view_check_or_cancel_at_my_place"            app:layout_constraintTop_toBottomOf="@+id/profile_page_image_view_check_or_cancel_at_my_place" />        <TextView            android:id="@+id/textView12"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_marginRight="5dp"            android:layout_weight="2"            android:fontFamily="@font/montserrat_bold"            android:gravity="center"            android:paddingHorizontal="@dimen/StandardPadding"            android:text="Zoom Meeting"            android:textColor="#000"            app:layout_constraintBottom_toBottomOf="@+id/profile_page_image_view_check_or_cancel_zoom"            app:layout_constraintEnd_toEndOf="@+id/textView3"            app:layout_constraintStart_toStartOf="@+id/textView3"            app:layout_constraintTop_toTopOf="@+id/profile_page_image_view_check_or_cancel_zoom" />        <TextView            android:id="@+id/textView13"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_marginTop="16dp"            android:background="@color/colorPrimaryDark"            android:fontFamily="@font/montserrat_medium"            android:gravity="center"            android:text="Lessons Offered"            android:textColor="@color/white"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/textView12" />        <TextView            android:id="@+id/give_rate"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginEnd="16dp"            android:fontFamily="@font/montserrat_medium"            android:text="@string/give_rate"            android:textColor="@color/colorPrimaryDark"            app:layout_constraintBottom_toBottomOf="@+id/profile_page_rating_bar"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@+id/profile_page_rating_bar"            app:layout_constraintTop_toTopOf="@+id/profile_page_rating_bar" />    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/Recycler_View_TeacherProfile_LessonsOffered"        android:layout_width="match_parent"        android:layout_height="0dp"        android:layout_margin="5dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/sub_parent" />    <Button        android:id="@+id/buttonAboutMe"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:background="@color/colorPrimaryDark"        android:drawableLeft="@drawable/ic_left_arrow"        android:drawablePadding="5dp"        android:fontFamily="@font/montserrat_medium"        android:onClick="OnClick_ProfilePage_AboutMe"        android:padding="10dp"        android:text="About Me"        android:textAllCaps="false"        android:textColor="@color/white"        app:layout_constraintEnd_toStartOf="@+id/chat_with_teacher"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/profile_page_teacher_name" />    <Button        android:id="@+id/chat_with_teacher"        android:layout_width="wrap_content"        android:layout_height="0dp"        android:background="@color/colorPrimaryDark"        android:drawableLeft="@drawable/ic_chat"        android:drawablePadding="5dp"        android:fontFamily="@font/montserrat_medium"        android:onClick="OnClick_profile_button_start_chat"        android:padding="10dp"        android:text="Chat Now"        android:textAllCaps="false"        android:textColor="#fff"        app:layout_constraintBottom_toBottomOf="@+id/buttonAboutMe"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@+id/buttonAboutMe"        app:layout_constraintTop_toTopOf="@+id/buttonAboutMe" /></androidx.constraintlayout.widget.ConstraintLayout>